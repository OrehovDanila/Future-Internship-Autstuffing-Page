@import '~styles/_import'

.wrap
	display flex
	justify-content center
	align-items center
	padding 0px 60px
	&::-webkit-scrollbar
		display none
	@media screen and (max-width 1280px)
		padding 0px
	@media tablet
		justify-content flex-start
		overflow-x scroll
		flex-wrap nowrap
		-webkit-overflow-scrolling touch
		-ms-overflow-style none
		padding 0px
	@media mobile
		justify-content flex-start
		overflow-x scroll
		flex-wrap nowrap
		-webkit-overflow-scrolling touch
		-ms-overflow-style none
		padding 0px

.logo
	& + &
		margin-left 65px
		@media mobile
			margin-left 40px

	&__img
		@media mobile
			max-width unset
		@media tablet
			max-width unset
